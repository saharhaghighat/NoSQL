CREATE TABLE books_by_genre (
  genre text,
  book_id UUID,
  title text,
  author text,
  sale_date date,
  sale_price decimal,
  PRIMARY KEY (genre, sale_price, book_id)
) WITH CLUSTERING ORDER BY (sale_price DESC);

CREATE TABLE books_by_author (
  author text,
  book_id UUID,
  title text,
  genre text,
  sale_date date,
  sale_price decimal,
  PRIMARY KEY (author, sale_date, book_id)
) WITH CLUSTERING ORDER BY (sale_date DESC);

